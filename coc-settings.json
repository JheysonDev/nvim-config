{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql",
    "html",
    "yaml",
    "toml",
    "json",
    "python",
    "rust",
    "go"
  ],
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "/usr/bin/flake8",
  "python.linting.flake8Args": [
    "--max-line-length=120",
    "--ignore=E402,F841,F401,E302,E305,W503"
  ],
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/bin/black",
  "deno.lint": true,
  "deno.config": "./tsconfig.json",
  "deno.unstable": true,
  "deno.importMap": "./import_map.json",
  "explorer.width": 32,
  "explorer.quitOnOpen": true,
  "explorer.file.showHiddenFiles": false,
  "explorer.file.hiddenRules": {
    "extensions": ["o", "a", "obj", "pyc"],
    "filenames": ["\\.byebug+", "\\.DS_Store|"],
    "patternMatches": ["^\\.git$"]
  }
}
